<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.migu.income.dao.TMiguPriceConfigHistoryMapper" >
  <resultMap id="BaseResultMap" type="cn.migu.income.pojo.TMiguPriceConfigHistory" >
  	<result column="RN" property="rn" jdbcType="VARCHAR" />
    <result column="HISTORY_ID" property="historyId" jdbcType="VARCHAR" />
    <result column="PROJECT_ID" property="projectId" jdbcType="VARCHAR" />
    <result column="PROJECT_NAME" property="projectName" jdbcType="VARCHAR" />
    <result column="SUBMIT_AUDIT_TIME" property="submitAuditTime" jdbcType="VARCHAR" />
    <result column="PERFORM_AUDIT_TIME" property="performAuditTime" jdbcType="VARCHAR" />
    <result column="AUDIT_RESULT" property="auditResult" jdbcType="VARCHAR" />
    <result column="HANDLING_SUGGESTION" property="handlingSuggestion" jdbcType="VARCHAR" />
    <result column="PRICE_CONFIG_ID" property="priceConfigId" jdbcType="VARCHAR" />
  </resultMap>
  
  <select id="queryAllPriceConfigInfoHis" resultMap="BaseResultMap">
	    select t11.* from(select t1.*,rownum rn from (select t.*  from T_MIGU_PRICE_CONFIG_HISTORY t where 1=1 
	     		and PRICE_CONFIG_ID = #{0} order by SUBMIT_AUDIT_TIME desc
	     ) t1) t11 
  </select>
</mapper>